{
    "name": "redis-test-server",
    "version": "1.0.0",
    "description": "Redis cache testing with Express + TypeScript + Bun",
    "scripts": {
        "dev": "bun run --watch src/index.ts",
        "start": "prisma db push && bun run src/index.ts",
        "db:generate": "prisma generate",
        "db:push": "prisma db push"
    },
    "dependencies": {
        "dotenv": "^17.2.3",
        "express": "^4.18.2",
        "ioredis": "^5.3.2",
        "@prisma/client": "^5.7.1"
    },
    "devDependencies": {
        "@types/express": "^4.17.21",
        "bun-types": "latest",
        "@types/bun": "latest",
        "prisma": "^5.7.1"
    },
    "module": "src/index.ts",
    "type": "module",
    "private": true,
    "peerDependencies": {
        "typescript": "^5"
    }
}